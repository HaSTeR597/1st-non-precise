engine = pyttsx3.init()
engine.say(" Enter 1st number ")
engine.runAndWait()      
c = int(input("Enter 1st number : "))

engine = pyttsx3.init()
engine.say("Enter 2nd number ")
engine.runAndWait() 
j = int(input("Enter 2nd number : "))

print("The addtion of this 2 numbers is = ",c+j)
print("The sub of this 2 numbers is = ",c-j)
print("The div of this 2 numbers is = ",c/j)
print("The multiplication of this 2 numbers is = ",c*j)
print("The modulus of this 2 numbers is = ",c%j)
print()

