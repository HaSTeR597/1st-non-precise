engine = pyttsx3.init()
engine.say(" Answer this simple question.. what's 6+8 x 2? ")
engine.runAndWait()
  
top = input(" Ans this simple question.. what's 6+8 x 2? ")
print()

if top == '22':
    engine = pyttsx3.init()
    engine.say(" Good.. now I agree your intelligence.. and maybe , we will be good friends in future! ")
    engine.runAndWait()  
    print (" >   Good.. now I agree your intelligence.. and maybe , we will be good friends in future!   < ") 
else:

    engine = pyttsx3.init()
    engine.say(" Wrong answer! , Remember maths rule 'BODMAS'? , answer is 22 :) ")
    engine.runAndWait() 
    print (" >    Wrong answer! , Remember maths rule 'BODMAS'? , ans is 22 :)   <")
    
    
