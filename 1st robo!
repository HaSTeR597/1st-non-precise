import random
chars = "abcdefghijklmnopqrstuvwxyz1234567890!@#$%^&*()_+}|;'\[].,?<>L:"
print()

engine = pyttsx3.init()
engine.say(" Now, Allow me to find a new password for you , Enter the length of password,  upto which size you want? ")
engine.runAndWait() 

length= int(input("Now, Allow me to find a new password for facebook for you , Enter the length of password,  upto which size you want? (for eg. 7) : " ))
password=""

for a in range(length):
    password+=random.choice(chars)
    print(password)
print()

engine = pyttsx3.init()
engine.say(" Do you want alphabet passwords only? ")
engine.runAndWait() 

q = str(input("Do you want alphabet passwords only? (yes/no) : "))

if q == 'yes':
    
  import random
  chars = "abcdefghijklmnopqrstuvwxyzQWERTYUIOPASDFGHJKLZXCVBNM"
  print()
  engine = pyttsx3.init()
  engine.say(" Enter the length of password,  upto which size you want? ")
  engine.runAndWait() 
  length= int(input(" Enter the length of password,  upto which size you want? (for eg. 7) : " ))
  password=""

  for a in range(length):
      password+=random.choice(chars)
  print(password)

else:

    engine = pyttsx3.init()
    engine.say(" Ohk your choice! ")
    engine.runAndWait() 
    print("Ok yrr")

time.sleep(1.5)   
print()


