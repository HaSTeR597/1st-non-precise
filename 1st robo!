if clor == 'student':
    engine = pyttsx3.init()
    engine.say(" Ok so you are learning! ")
    engine.runAndWait()
    print("Ok so you are learning!")

elif clor == 'teacher':
    engine = pyttsx3.init()
    engine.say(" Welcome sir/mam ")
    engine.runAndWait()
    print(" >>>  Welcome sir/mam  <<<")
  
else:
    engine = pyttsx3.init()
    engine.say(" please enter the keywords correctly as they are case sensitive, kindly restart(recompile) the program! ")
    engine.runAndWait()
    print("please enter the keywords correctly as they are case sensitive, kindly restart(recompile) the program!")

